{
  "name": "react-capstone-project",
  "version": "1.0.0",
  "description": "This repo contains final project requirements and template for [GridU React course](https://gridu.litmos.com/home/LearningPath/26572?r=False&ts=637785982384795974). You should fork it and complete the task in the forked repository.",
  "scripts": {
    "start-client": "cd ./twitter-clone && react-scripts start build",
    "build": "cd ./twitter-clone && react-scripts build && (cd ../server && npm i)",
    "test": "cd ./twitter-clone && react-scripts test",
    "eject": "cd ./twitter-clone && react-scripts eject",
    "lint": "eslint './twitter-clone/src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './twitter-clone/src/**/*.{js,ts,tsx}'",
    "format": "prettier --write './twitter-clone/src/**/*.{js,ts,tsx,css,md}' --config ./.prettierrc",
    "start": "cd ./server && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohalytska-ui/react-capstone-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ohalytska-ui/react-capstone-project/issues"
  },
  "homepage": "./twitter-clone",
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons": "^4.8.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "antd": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.3",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.7.1"
  }
